<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.ScrollPane?>

<Tab xmlns="http://javafx.com/javafx"
     xmlns:fx="http://javafx.com/fxml"
     closable="false" text="Эпизоды"
     styleClass="hide"
     fx:controller="com.cinema.javafx.controller.movie.SeasonsController">
    <StackPane fx:id="seriesPane">
        <ScrollPane>
            <TilePane fx:id="seasonsPane" hgap="4.0" prefColumns="3" prefRows="3" vgap="4.0">
                <padding>
                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                </padding>
            </TilePane>
        </ScrollPane>
    </StackPane>
</Tab>

